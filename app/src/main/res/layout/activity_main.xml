        android:layout_centerHorizontal="true"
        android:layout_marginStart="93dp"
        android:layout_marginLeft="93dp"
        android:layout_marginTop="172dp"
        android:text="@string/connect"
        android:textSize="30sp"
        />

    <SeekBar
